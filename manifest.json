{
"manifest_version": 2,
"name":"Explore",
"description":"Extract Html and Css",
"version":"1.0.0",
"icons":{"128":"icon.png"},
"browser_action":{
	"default_icon":"icon.png",
	"default_html":"popup.html"
},
"content_scripts":[
	{
		"matches":[
			"<all_urls>"
		],
		"js":["popup.js"]
	}

	
],
"permissions":["activeTab"]
}